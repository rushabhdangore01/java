package com.stream.java8;

class Array   
{   
      public static void main(String[] args)   
      {   
            int[] n={12,24,2,89,34,45};   
       }

 

 

         static void display(int n[])   
      {   
         for(int i=0; i<n.length;i++)   
             System.out.print(n[i] + " ");   
      }
}